            <div class="col-lg-5 col-sm-5 padbot20" data-animated="fadeIn">
                <h2>Напишите нам</h2>

                <!-- CONTACT FORM -->
                <div class="contact_form top_form">
                    <div id="note"></div>
                    <div id="fields">
                        <form id="ajax-contact-form" action="">
