                            <input type="text" name="name" value="" placeholder="Ваше имя" />
                            <label>Email</label>
                            <input type="text" name="email" value="" placeholder="Ваш Email" />
                            <label>Subject</label>
                            <textarea name="message" placeholder="Ваше сообщение"></textarea><br>
                            <input class="contact_btn sent_btn" type="submit" value="Отправить сообщение" />
                        </form>
                    </div>
                </div><!-- //CONTACT FORM -->
            </div>
            <div class="col-lg-7 col-sm-7 padbot20" data-animated="fadeIn">
